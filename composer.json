		"yii2bundle/yii2-tool": "dev-master",
		"yii2bundle/yii2-vendor": "dev-master",
		"yii2bundle/guide-misc": "dev-master",
		"yii2bundle/guide-team": "dev-master",
		"yii2guide/markdown": "dev-master",
		"yii2guide/perfect-code": "dev-master"

	},
	"config": {
		"secure-http": false,
		"process-timeout": 1800
	},
	"extra": {
		"installer-types": [
			"bower-asset",
			"npm-asset"
		],
		"installer-paths": {
			"vendor/{$vendor}/{$name}/": [
				"type:bower-asset",
				"type:npm-asset"
			]
		}
	},
